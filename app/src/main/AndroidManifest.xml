<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.healthit"
    android:installLocation="preferExternal">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.NoActionBar">
        <activity android:name=".LogIn"></activity>
        <activity android:name=".Register" />
        <activity android:name=".Tracker" />
        <activity android:name=".Web" />
        <activity
            android:name=".chart"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity
            android:name=".dialog"
            android:theme="@style/Theme.AppCompat.Dialog" />
        <activity android:name=".ProteinIntake" />
        <activity android:name=".idealWeight" />
        <activity android:name=".BMR" />
        <activity android:name=".BMI" />
        <activity android:name=".Home" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>